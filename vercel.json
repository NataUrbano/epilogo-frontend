{
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "s-maxage=1, stale-while-revalidate" }
      ]
    }
  ],
  "buildCommand": "ng build",
  "outputDirectory": "dist/epilogo-frontend",
  "installCommand": "npm install"
}